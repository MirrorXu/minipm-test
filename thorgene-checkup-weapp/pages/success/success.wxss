page{
  background: #f2f2f2;
}
.success_page{
  padding-top: 200rpx;
  width: 100%;
  position: relative;
}
.success_page image{
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto 30rpx;
  display: block;
}
.success_page text{
  font-size: 34rpx;
  color: #666;
  line-height: 54rpx;
  width: 660rpx;
  text-align: center;
  margin: 0 auto;
  display: block;
}
.success_page button{
  width: 610rpx;
  height: 98rpx;
  line-height: 98rpx;
  margin: 160rpx auto 0;
}
.success_page text.success_txt{
  width: 100%;
  font-size: 36rpx;
  color: #333;
  line-height: 48rpx;
  text-align: center;
  display: block;
  margin-bottom: 80rpx;
}