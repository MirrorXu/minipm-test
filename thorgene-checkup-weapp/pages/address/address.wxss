.nav_list{
  width: 100%;
  height: 88rpx;
  position: fixed;
  top: 0;
  left: 0;
  border-bottom: 1rpx solid #d7d7d7;
  overflow: hidden;
  background: #fff;
  z-index: 9;
}
.nav_list .nav_item{
  float: left;
  width: 50%;
  box-sizing: border-box;
  line-height: 88rpx;
  font-size: 30rpx;
  text-align: center;
  color: #666;
  background: #fff;
}
.nav_list text.selected{
  color: #2a88e9;
}
.select{
  width: 13rpx;
  height: 8rpx;
  display: inline-block;
  vertical-align: middle;
  text-align: center; 
  margin-left: 20rpx;
}
.content_list{
  width: 100%;
  height: auto;
  position: fixed;
  top: 90rpx;
  left: 0;
  background: #fff;
  z-index: 8;
}
.content_item{
  width: 700rpx;
  margin: 0 auto;
  overflow: hidden;
  padding: 20rpx 0;
  display: block;
}
.area_list{
  display: none;
}
.area_list li{
  width: 200rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  display: block;
  background: #f5f5f5;
  color: #666;
  font-size: 30rpx;
  float: left;
  margin: 20rpx 16.6rpx;
}
.area_list li.selected{
  background: #2a88e9;
  color: #fff;
}
.institutions_list{
  display: none;
}
.institutions_list li{
  width: 300rpx;
  height: 70rpx;
  text-align: center;
  line-height: 70rpx;
  display: block;
  background: #f5f5f5;
  color: #666;
  font-size: 30rpx;
  float: left;
  margin: 20rpx 25rpx;
}
.institutions_list li.selected{
  background: #2a88e9;
  color: #fff;
}
.item_list{
  margin: 100rpx auto 0;
  width: 710rpx;
  padding-bottom: 170rpx;
}
.item{
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  border-bottom: 1rpx solid #d7d7d7;
}
.item .mapBtn{
  padding: 20rpx 30rpx;
}
.item .mapBtn image{
  width: 26rpx;
  height: 40rpx;
  display: block;
}
.radio{
  padding: 20rpx 0;
  position: relative;
  display: block;
}
.radio radio{
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  top: 20rpx;
  left: 25rpx;
}
.radio view{
  width: 540rpx;
  margin-left: 90rpx;
}
.radio view .name{
  font-size: 34rpx;
  line-height: 46rpx;
  color: #333;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.radio view .address{
  font-size: 32rpx;
  line-height: 42rpx;
  color: #999;
  margin-top: 10rpx;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.radio view .icon{
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin-left: 1rpx;
  margin-top: 10rpx;
}
.radio view .icon text{
  height: 38rpx;
  line-height: 38rpx;
  padding: 0 10rpx;
  border: 1rpx solid #a6a6a6;
  color: #a6a6a6;
  border-radius: 5rpx;
  font-size: 28rpx;
  margin: 10rpx 10rpx 0 0;
}
.radio view .icon text.hide{
  display: none;
}
.item_list:last-child .item{
  border-bottom: none;
}
.page{
  padding-bottom: 40rpx;
}
.radio radio{
  opacity: 0;
}
.radio_select{
  width: 40rpx;
  height: 40rpx;
  display: block;
  position: absolute;
  top: 26rpx;
  left: 30rpx;
}
.btnBox{
  width: 100%;
  height: 130rpx;
  padding-top: 20rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  background: #fff;
}
.btnBox button.next_btn{
  width: 660rpx;
  height: 100rpx;
  line-height: 100rpx;
  margin: 0 auto;
  display: block;
}
.mask{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.1);
  position: fixed;
  top: 0;
  left: 0;
  display: block;
}
.default_box{
  width: 100%;
  height: 100%;
  background: #f2f2f2;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 7;
  display: none;
}
.default_box image{
  width: 100%;
  height: 500rpx;
  display: block;
  margin-top: 200rpx;
}
.default_box text{
  width: 100%;
  font-size: 28rpx;
  color: #999;
  line-height: 40rpx;
  text-align: center;
  display: block;
}
.hide{
  display: none;
}
.show{
  display: block;
}